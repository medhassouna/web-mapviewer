    "animate.css": "^4.1.1",
    "axios": "^0.20.0",
    "bootstrap": "^4.5.3",
    "core-js": "^3.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "maplibre-gl": "^1.13.0-rc.3",
    "mobile-device-detect": "^0.4.3",
    "moment": "^2.29.1",
    "ol": "^6.4.3",
    "popper.js": "^1.16.1",
    "proj4": "^2.6.1",
